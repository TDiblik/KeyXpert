{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "ui",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "message": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "keyxpert.tomasdiblik.cz",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "title": "KeyXpert",
        "theme": "Dark",

        "fullscreen": false,
        "resizable": true,
        "maximized": false,
        "focus": true,
        "alwaysOnTop": false,
        "decorations": true,

        "width": 750,
        "height": 650,

        "maxWidth": 1350,

        "minWidth": 750,
        "minHeight": 650
      }
    ]
  }
}
